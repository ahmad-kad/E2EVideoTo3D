# Environment configuration for 3D Reconstruction Pipeline
# Copy this file to .env and adjust the values

# Base paths
PROJECT_PATH=/opt/airflow/data
#INPUT_PATH=${PROJECT_PATH}/input
#OUTPUT_PATH=${PROJECT_PATH}/output
#VIDEO_PATH=${PROJECT_PATH}/videos

# COLMAP configuration
COLMAP_PATH=colmap
USE_GPU=auto  # auto, true, or false
QUALITY_PRESET=medium  # low, medium, or high

# S3/MinIO configuration
S3_ENABLED=false
S3_ENDPOINT=http://minio:9000
S3_BUCKET=models
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_REGION=us-east-1

# Notification configuration
SLACK_WEBHOOK=
ENABLE_EMAIL=false
NOTIFICATION_EMAIL= 